<section class="px-4 pb-8 mb-8">
  <div class="wrapper">
    
    <div
      class="bg-white shadow border border-gray-light rounded-lg overflow-hidden"
    >
      <span
        class="block font-base-semibold text-sm bg-navy text-white uppercase tracking-wide border-b-2 border-green p-4"
        >Castle Counselling Services can help with:</span
      >

      <ul class="ccs-list [ list-reset ][ md:flex md:flex-wrap ]">
        {% for item in services %}
        <li
          class="border-b border-gray-light flex items-center justify-between [ md:w-1/2 ][ lg:w-1/3 ]"
        >
          <a
            href="/{{ item.uri }}"
            class="[ block flex items-center justify-between w-full ] [ no-underline text-gray hover:text-green ][ p-4 ]"
          >
            <span>{{ item.title }}</span>
            <svg
              class="flex-no-shrink ml-4"
              style="height: 13px; width: 13px"
              viewBox="0 0 13 13"
              xmlns="http://www.w3.org/2000/svg"
            >
              <title>Arrow Right Icon</title>
              <path
                d="M12.128 3.237A6.473 6.473 0 0 0 9.763.872 6.358 6.358 0 0 0 6.5 0C5.32 0 4.233.29 3.237.872A6.471 6.471 0 0 0 .872 3.237 6.358 6.358 0 0 0 0 6.5c0 1.18.29 2.267.872 3.263a6.473 6.473 0 0 0 2.365 2.365A6.359 6.359 0 0 0 6.5 13c1.18 0 2.267-.29 3.263-.872a6.471 6.471 0 0 0 2.365-2.365A6.36 6.36 0 0 0 13 6.5a6.36 6.36 0 0 0-.872-3.263zM9.911 6.881l-3.843 3.842a.52.52 0 0 1-.38.161.52.52 0 0 1-.381-.16l-.864-.864a.52.52 0 0 1-.16-.381.52.52 0 0 1 .16-.38L7.042 6.5 4.443 3.902a.52.52 0 0 1-.16-.381.52.52 0 0 1 .16-.381l.864-.864a.52.52 0 0 1 .38-.16.52.52 0 0 1 .381.16L9.911 6.12a.52.52 0 0 1 .16.38.52.52 0 0 1-.16.382z"
                fill="#6DC9C9"
                fill-rule="nonzero"
              />
            </svg>
          </a>
        </li>
        {% endfor %}
      </ul>
    </div>
  </div>
</section>