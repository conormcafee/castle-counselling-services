---
layout: layout.njk
pagination:
    data: services
    size: 1
    alias: service
permalink: "services/{{ service.title | slug }}/"
title: service.title
---

<section class="hero gradient-light [ px-4 pt-4 pb-8 ][ bg-pink-light bg-cover bg-center bg-no-repeat ]" style="background-image: url('/assets/images/hero/hero-graphic.jpg">
	<div class="wrapper relative z-10">
    <div class="md:pt-2">
			<nav class="mb-8">
				<ul class="list-reset [ sm:flex flex-wrap items-center ]">
					<li class="inline-block sm:inline-flex items-center">
						<a href="/" class="text-gray hover:text-green font-base-semibold text-sm no-underline">Home</a>
						<span class="text-xs [ ml-2 ]">></span>
					</li>

					<li class="inline-block m:inline-flex items-center [ ml-2 ]">
						<a href="/services" class="text-gray hover:text-green font-base-semibold text-sm no-underline">Services</a>
						<span class="text-xs [ ml-2 ]">></span>
					</li>
					
					<li class="block [ sm:ml-2 ] sm:inline-flex items-center">
						<span class="text-green font-base-semibold text-sm">{{ service.title | safe }}</span>
					</li>
				</ul>
			</nav>
            
			<h1 class="font-heading leading-tight text-navy text-4xl max-w-md [ md:text-5xl ]">{{ service.title | safe }}</h1>
            
			<div class="cms-content mt-4 max-w-md">
				{{ service.field_introduction | safe }}
			</div>
		</div>
	</div>
</section>

<section class="px-4 pb-8 mb-4">
	<div class="wrapper">
		<a href="/get-in-touch" class="[ block no-underline ][ text-center md:text-left ][ p-8 ][ bg-navy opacity-99 rounded-lg ][ sm:flex sm:items-center md:justify-between ]">
    	<span class="[ block ][ font-base-semibold text-center text-white leading-none tracking-wide [ text-2xl md:text-4xl ] ]">Book your counselling session today</span>
			<span class="button button-green [ inline-block relative px-8 py-2 ][ rounded-full ][ font-base-semibold ][ no-underline ][ bg-green hover:bg-green-dark ][ text-white ][ mt-6 [ sm:mt-0 ] ]">Get in Touch</span>
		</a>
  </div>
</section>

<section class="px-4 pb-8">
	<div class="wrapper">
		<article class="cms-content cms-content--two-col [ max-w-lg ]">
			{{ service.field_body | safe }}
		</article>
	</div>
</section>

